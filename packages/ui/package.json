{
    "name": "@semoss/ui",
    "version": "1.0.0-beta.3",
    "description": "",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "files": [
        "src",
        "dist"
    ],
    "scripts": {
        "start": "pnpm run storybook",
        "dev": "run-p build:dev storybook",
        "build": "rollup -c",
        "build:dev": "rollup -w -c",
        "storybook": "cross-env NODE_OPTIONS=--max_old_space_size=8192 storybook dev -p 6006",
        "lint": "nx eslint:lint",
        "lint:fix": "nx eslint:lint --fix",
        "format": "prettier . --check --plugin-search-dir=.",
        "format:fix": "prettier . --write --plugin-search-dir=.",
        "fix": "run-s format:fix lint:fix",
        "build-storybook": "storybook build"
    },
    "nx": {
        "targets": {
            "build-nx": {
                "executor": "@nx/rollup:rollup",
                "cache": true,
                "inputs": [
                    "production",
                    "^production"
                ],
                "options": {
                    "main": "${projectRoot}/src/index.ts",
                    "tsConfig": "packages/ui/tsconfig.json",
                    "outputPath": "packages/ui/dist",
                    "rollupConfig": "packages/ui/rollup.config.js"
                },
                "configurations": {}
            }
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@babel/core": "7.22.5",
        "@emotion/react": "11.11.1",
        "@emotion/styled": "11.11.0",
        "@mui/icons-material": "5.11.16",
        "@mui/material": "5.13.5",
        "@mui/x-date-pickers": "6.8.0",
        "@mui/x-tree-view": "^6.0.0-alpha.2",
        "cross-env": "7.0.3",
        "dayjs": "1.11.8",
        "deepmerge": "4.3.1",
        "mui-phone-number": "3.0.3",
        "typescript": "5.1.3"
    },
    "devDependencies": {
        "@babel/preset-env": "7.22.5",
        "@babel/preset-react": "7.22.5",
        "@babel/preset-typescript": "7.21.5",
        "@rollup/plugin-commonjs": "25.0.7",
        "@rollup/plugin-node-resolve": "15.2.3",
        "@rollup/plugin-typescript": "11.1.6",
        "@storybook/addon-essentials": "7.0.18",
        "@storybook/addon-interactions": "7.0.18",
        "@storybook/addon-links": "7.0.18",
        "@storybook/addon-storysource": "7.0.23",
        "@storybook/addon-styling": "1.3.0",
        "@storybook/blocks": "7.0.18",
        "@storybook/react": "7.0.18",
        "@storybook/react-webpack5": "7.0.18",
        "@storybook/testing-library": "0.0.14-next.2",
        "@types/react": "17.0.62",
        "@types/react-dom": "17.0.20",
        "@typescript-eslint/eslint-plugin": "5.59.11",
        "@typescript-eslint/parser": "5.59.11",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-prettier": "3.4.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.8",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-dropzone": "14.2.3",
        "rollup": "2.79.1",
        "rollup-plugin-bundle-size": "1.0.3",
        "rollup-plugin-delete": "2.0.0",
        "rollup-plugin-postcss": "4.0.2",
        "storybook": "7.0.18",
        "tslib": "2.5.3"
    },
    "peerDependencies": {
        "@mui/icons-material": "5.11.16",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    }
}
