{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Init",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/bin/run.js",
            "args": ["init", "-n=test", "-e=./test/.env.local"],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Deploy",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/bin/run.js",
            "args": ["deploy", "-e=./test/.env.local", "-r=./test/"],
            "outputCapture": "std"
        }
    ]
}
