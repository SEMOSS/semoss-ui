{
  "name": "@semoss/cli",
  "version": "1.0.0-beta.2",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "exports": "./lib/index.js",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "nx eslint:lint --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "nx eslint:lint --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier . --check --plugin-search-dir=.",
    "format:fix": "prettier . --write --plugin-search-dir=.",
    "fix": "run-s format:fix lint:fix"
  },
  "bin": {
    "@semoss/cli": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^3.7.0",
    "@semoss/sdk": "^1.0.0-beta.15",
    "adm-zip": "^0.5.10",
    "dotenv": "^16.0.1",
    "force": "^0.0.3",
    "glob": "^10.3.10",
    "listr": "^0.14.3"
  },
  "devDependencies": {
    "@semoss/sdk": "workspace:*",
    "@types/adm-zip": "^0.5.3",
    "@types/listr": "^0.14.7",
    "@types/node": "^18",
    "add": "^2.0.6",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "oclif": "^4.0.3",
    "prettier": "2.4.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "oclif": {
    "bin": "@semoss/cli",
    "dirname": "@semoss/cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  }
}
