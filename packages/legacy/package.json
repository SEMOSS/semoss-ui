    "fix": "run-s format:fix lint:fix",
    "stats:dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 webpack --config webpack.dev.js --profile --json > webpack-stats.json",
    "stats:prod": "cross-env NODE_OPTIONS=--max-old-space-size=8192 webpack --config webpack.prod.js --profile --json > webpack-stats.json"
  },
