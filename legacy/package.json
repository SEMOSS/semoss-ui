{
    "version": "1.0.0",
    "description": "SEMOSS Analytics Platform",
    "main": "index.js",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "start": "pnpm run dev",
        "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 webpack --watch --progress --config webpack.dev.js",
        "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 webpack --config webpack.prod.js",
        "build:docs": "node ./makedocs.js",
        "check-types": "tsc",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier . --check --plugin-search-dir=.",
        "format:fix": "prettier . --write --plugin-search-dir=.",
        "fix": "run-s format:fix lint:fix",
        "stats:dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 webpack --config webpack.dev.js --profile --json > webpack-stats.json",
        "stats:prod": "cross-env NODE_OPTIONS=--max-old-space-size=8192 webpack --config webpack.prod.js --profile --json > webpack-stats.json"
    },
    "author": "",
    "license": "GPL-2.0",
